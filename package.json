{
  "name": "noteapp",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "express": "^4.16.2",
    "neo4j-driver": "^1.5.3",
    "nodemon": "^1.14.12",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "yarn": "^1.3.2",
    "axios": "^1.6.2",
    "dnsregistrar": "0.0.51"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "engines": {
    "node": "8.9.3"
  }
}
